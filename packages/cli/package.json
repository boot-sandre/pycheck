{
  "name": "@pycheck/cli",
  "version": "0.7.0",
  "description": "An opinionated code quality checker for Python, terminal client",
  "main": "bin/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emencia/pycheck.git"
  },
  "keywords": [
    "python",
    "type_check"
  ],
  "scripts": {
    "debug": "ts-node src/index.ts",
    "dev": "yarn run ts-node src/index.ts",
    "build": "tsc -p ."
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "ini": "^4.0.0"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/node": "^16.18.23",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "bin"
  ],
  "bin": {
    "pycheck": "bin/index.js"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}